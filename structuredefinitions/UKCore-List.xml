  <name value="UKCoreList" />
  <title value="UK Core List" />
  <status value="draft" />
  <date value="2019-11-22" />
  <publisher value="NHS Digital" />
  <contact>
    <name value="Interoperability Team" />
    <telecom>
      <system value="email" />
