        <targetProfile value="https://fhir.nhs.uk/R4/StructureDefinition/UKCore-RelatedPerson" />
        <targetProfile value="https://fhir.nhs.uk/R4/StructureDefinition/UKCore-Organization" />
      </type>
      <type>
        <code value="string" />
